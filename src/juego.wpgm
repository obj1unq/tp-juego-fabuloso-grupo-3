import wollok.game.*
import objects.*
import cosasQueCaen.*

program juego{
	game.title("veremos")
	game.height(10)
	game.width(10)
	game.ground("suelo.png")
	
	//Visuales
	game.addVisualCharacter(personaje)
	game.addVisual(proyectil)
	game.addVisual(bomba)
	game.addVisual(botiquin)
	
	game.onCollideDo(personaje, {otro => otro.colisionoCon(personaje)})
	
	//Teclado
	keyboard.v().onPressDo({game.say(personaje, personaje.vida().toString())})
	
	game.start()
}
