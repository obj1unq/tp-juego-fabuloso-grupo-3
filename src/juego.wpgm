import wollok.game.*
import personaje.*
import cosasQueCaen.*
import cronometro.*
import lanzaProyectiles.*
import guarda.*
import vida.*
import visualScreen.*

program juego{
	
	// JUEGO
	game.title("El guachin")
	game.height(8)
	game.width(8)
	game.ground("pastito.png")
	game.cellSize(64)
	
	//PRESENTACION
	visualScreen.inicializar()
	keyboard.enter().onPressDo({
		if (visualScreen.inicializada()) {
			visualScreen.quitar()
		}
		visualScreen.newGame()
	})
	
	//START
	game.start()
}